<script>
  var category_list = {$category_list|default=""|json_encode};
  var vo = {$vo|default=""|json_encode};
  var config_data = {$config_data|default=""|json_encode};
 $(function(){
  $('#search_body').hide()
  $('#form-edit').height($(window).height())
  get_form_list(config_data.edit_setting,'form-edit');
  for(var key in vo){
    switch( $('input[name="'+key+'"],select[name="'+key+'"],textarea[name="'+key+'"],select[name="'+key+'[]"],select[name="'+key+'"]').attr('data-edit_type')){
      case 'text':
      case 'number':
      case 'hidden':
      case 'password':
      $('input[name="'+key+'"]').val(vo[key]);
      break;
      case 'rich_text':
      $('#'+key+'_rich').text(vo[key]);
      break;
      case 'textarea':
      $('textarea[name="'+key+'"]').text(vo[key]);
      break;
      case 'date_choose':
      $('input[name="'+key+'"]').val(formatDate(vo[key],'yyyy-MM-dd hh:mm'));
       break;
      case 'date_range':
       $('input[name="'+key+'"]').datepicker('setDate',formatDate(vo[key],'yyyy-MM-dd'));
      break;
      case 'select_singer':
       $('#'+key+'_select2').val([vo[key]]).trigger("change");
      break;
      case 'select_multiple':
       $('#'+key+'_select2').val([vo[key]]).trigger("change");
      break;
      case 'radio':
       $('input[name="'+key+'"][value="'+vo[key]+'"]').attr('checked',true);
       break;
      case 'checkbox':
      if(vo[key].indexOf(',') == -1){
       $('input[name="'+key+'"][value="'+vo[key]+'"]').attr('checked',true);
      }else{
        var key_list = vo[key].split(',');
        for(var keys in key_list){
          $('input[name="'+key+'"][value="'+key_list[keys]+'"]').attr('checked',true);
        }
      }
      break;
    }
 })
 function newCustom(id,data){

}
</script>
