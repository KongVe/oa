<layout name='Layout/m_ace_layout' />

<style>
.showBottomBox{cursor: pointer;}
.list-block{
	margin:0;
}
</style>

<!-- <link rel="stylesheet" type="text/css" href="__PUBLIC__/css/boostarap.min.css">
<link rel="stylesheet" type="text/css" href="__PUBLIC__/boostarap-select"/>
