/*
手机版首页样式
*/
body{
  font-family: "Helvetica Neue",Helvetica,sans-serif;
}
img.icon.icon-ChatHead {
    width: 1.45rem;
    height: 1.45rem;
  }

  .item-content .success .badge {
    background: red;
    color:#FFFFFF;
  }

  .panel-left select option {
    color: #000;
  }
  .icon-home:before {
    content: "\e602";
  }
  .icon-star:before {
    content: "\e60e";
  }
nav ul{
margin: 0;
padding: 0;
 list-style:none; /* 去掉ul前面的符号 */
 width: auto;
}

/*未展开子菜单*/
.tab-item>div{
    position:relative;
    -webkit-box-sizing: border-box;
}
.tab-item>.new-tab{
  position: absolute;
    bottom: 2.5rem;
    z-index: -1;
    width: 100%;
    list-style: none;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: none;
   border-radius: 0.55rem;
    background: #f7f7f8;
}
.tab-item>ul li{
  float: none;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    line-height: 2rem;
    background: -webkit-gradient(linear, 0 0, 100% 0, from(rgba(194,194,194,0.8)), to(rgba(194,194,194,0.8)), color-stop(50%, rgba(194,194,194,0.8)));
    background-size: 80% 1px;
    background-repeat: no-repeat;
    background-position: center bottom;
    color: #929292;
}
.tab-item>.new-tab>.actives{
color:#0c0c0c;
}
.tab-item.active>.new-tab{
  display: block;
}
.tab-item.active>.new-tab.show{
  display: block;
}
.tab-item.active>.new-tab.hide{
  display: none;
}
#nav-float {
    position: fixed;
    right: .2rem;
    bottom: .2rem;
    width: 2.5rem;
    background: #fff;
    height: 2.4rem;
    z-index: 9999;
    border: 1px solid #dedede;
    border-radius: 1.25rem;
    -moz-border-radius: 1.25rem;
    -webkit-border-radius: 1.25rem;
    text-align: center;
}
#nav-overlady{
  position: absolute;
    top: 0;
    left: 0;
    z-index: 10000;
    width: 100%;
    height: 100%;
       visibility: visible;
    opacity: 1;
    background: rgba(0, 0, 0, .4);
    -webkit-transition-duration: 400ms;
    -o-transition-duration: 400ms;
    transition-duration: 400ms;
}
#nav-overlady.hide{
  display: none;
}
#nav-menu-list{
  display: none;
  position: fixed;
  height: auto;
  right: .25rem;
  width: 100%;
  z-index: 10001

}
#nav-menu-list ul{
  height: 100%;
  text-align: center;

}
.navbar-default {
    background-color: #f8f8f8;
    border-color: #e7e7e7;
}
.navbar:before, .navbar:after {
    display: table;
    content: " ";
}
.navbar {
    position: relative;
    z-index: 1000;
    min-height: 1.5rem;
    margin-bottom: .5rem;
    border: 1px solid transparent;
}
.navbar-collapse:before, .navbar-collapse:after {
    display: table;
    content: " ";
}
#nav-menu-list ul {
    height: 100%;
}
.navbar-nav {
    margin: 7.5px -15px;
}
.nav {
    padding-left: 0;
    margin-bottom: 0;
    list-style: none;
}
.nav:before, .nav:after {
    display: table;
    content: " ";
}
.nav:after {
    clear: both;
}
.nav:before, .nav:after {
    display: table;
    content: " ";
}
.nav:after {
    clear: both;
}
.nav:before, .nav:after {
    display: table;
    content: " ";
}
.nav>li {
    position: relative;
    display: block;
}
.navbar-default .navbar-nav>li>a {
    color: #777;
}
.navbar-nav>li>a {
    padding-top: 10px;
    padding-bottom: 10px;
    line-height: 20px;
}

.nav>li>a {
    position: relative;
    display: block;
    padding: 10px 15px;
}
.nav .caret {
    border-top-color: #428bca;
    border-bottom-color: #428bca;
}
.caret {
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: 2px;
    vertical-align: middle;
    border-top: 4px solid #000;
    border-right: 4px solid transparent;
    border-bottom: 0 dotted;
    border-left: 4px solid transparent;
    content: "";
}
.navbar-nav>li>.dropdown-menu {
    margin-top: 0;
    border-top-right-radius: 0;
    border-top-left-radius: 0;
}
.dropdown-menu {
    position: absolute;
    top: 100%;
    left: 0;
    z-index: 1000;
    display: none;
    float: left;
    min-width: 160px;
    padding: 5px 0;
    margin: 2px 0 0;
    font-size: 14px;
    list-style: none;
    background-color: #fff;
    border: 1px solid #ccc;
    border: 1px solid rgba(0,0,0,0.15);
    border-radius: 4px;
    -webkit-box-shadow: 0 6px 12px rgba(0,0,0,0.175);
    box-shadow: 0 6px 12px rgba(0,0,0,0.175);
    background-clip: padding-box;
}
.navbar-nav .open .dropdown-menu {
    position: static;
    float: none;
    width: auto;
    margin-top: 0;
    background-color: transparent;
    border: 0;
    box-shadow: none;
}
.navbar-default .navbar-nav>.open>a .caret, .navbar-default .navbar-nav>.open>a:hover .caret, .navbar-default .navbar-nav>.open>a:focus .caret {
    border-top-color: #555;
    border-bottom-color: #555;
}
.navbar-nav>li>.dropdown-menu {
    margin-top: 0;
    border-top-right-radius: 0;
    border-top-left-radius: 0;
}
.navbar-nav>li>.dropdown-menu {
    margin-top: 0;
    border-top-right-radius: 0;
    border-top-left-radius: 0;
}
.open>.dropdown-menu {
    display: block;
}
.dropdown-menu>li>a {
    display: block;
    padding: 3px 20px;
    clear: both;
    font-weight: normal;
    line-height: 1.428571429;
    color: #333;
    white-space: nowrap;
}