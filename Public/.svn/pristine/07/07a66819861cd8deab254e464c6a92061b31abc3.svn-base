          a = '';
        }
        if(a.visualLength()>len_arr_list)
       len_arr_list = a.visualLength();
     if(obj.label.visualLength()>len_arr_list){
      len_arr_list = obj.label.visualLength();
     }
      })
      obj['width'] = len_arr_list+1
    }
      // obj['width'] = 100
      colModel.push(obj);
    })
   Get_Grid(table,data,pager);
}
