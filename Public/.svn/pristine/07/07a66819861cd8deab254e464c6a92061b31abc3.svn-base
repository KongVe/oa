String.prototype.visualLength = function()
{
        var ruler = $("#len_arr");
        ruler.show();
        ruler.text(this);
        var i = ruler[0].offsetWidth;
        ruler.hide();
        return i;
}
function clomodel(table ,data,pager){
  $.each(config_data.title_list,function(i,v){
